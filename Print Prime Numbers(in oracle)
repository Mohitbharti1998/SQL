SET SERVEROUTPUT ON;
DECLARE
 COUNTER NUMBER;
 OUTPUT CLOB;
 K NUMBER;
 
 BEGIN 
    OUTPUT := '';
    FOR N IN 2 .. 1000 LOOP
    COUNTER := 0;
    K := FLOOR(N/2);
    
    FOR M IN 2 .. K LOOP
    IF( MOD(N,M) = 0) THEN
        COUNTER := 1;
        END IF;
    END LOOP;
    
    IF(COUNTER = 0) THEN 
    OUTPUT := OUTPUT || N || '&';
    END IF;
    END LOOP;
    DBMS_OUTPUT.PUT_LINE(SUBSTR(OUTPUT,0,length(OUTPUT)-1));
END;
/
